func run() -> void:
	clear_messages()
	emit_signal(\"highlight_line\", 0)
	yield()
	for number in range(3):
		emit_signal(\"highlight_line\", 1)
		emit_signal(\"arrow_animation\", [16 + (3*number),1], [5,6])
		yield()
		print_output([number])
		emit_signal(\"highlight_line\", 2)
		yield()
"

